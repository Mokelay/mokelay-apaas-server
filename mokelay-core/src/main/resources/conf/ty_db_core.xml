<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- API-->
    <class name="com.greatbee.core.bean.server.API" table="ty_api" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="`name`" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="alias" column="alias" type="string"/>
        <property name="createDate" column="createDate" type="java.util.Date"/>
        <property name="enable" column="enable" type="boolean"/>
        <property name="method" column="method" type="string"/>
        <property name="type" column="type" type="string"/>
        <property name="url" column="url" type="string"/>
        <property name="category" column="category" type="string"/>
        <property name="authType" column="authType" type="string"/>
        <property name="version" column="version" type="string"/>
        <property name="content" column="content" type="string"/>
    </class>

    <!-- OutputFieldDescribe-->
    <class name="com.greatbee.core.bean.server.FileStorage" table="ty_file_storage" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="oiAlias" column="`oiAlias`" type="string"/>
        <property name="serializeName" column="serializeName" type="string"/>
        <property name="originalName" column="originalName" type="string"/>
        <property name="fileSize" column="`fileSize`" type="long"/>
        <property name="fileType" column="`fileType`" type="string"/>
        <property name="contentType" column="`contentType`" type="string"/>
        <property name="fileUrl" column="`fileUrl`" type="string"/>
        <property name="uploadType" column="`uploadType`" type="string"/>
    </class>

    <!-- Log-->
    <class name="com.greatbee.core.bean.user.Log" table="ty_log" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="createDate" column="createDate" type="java.util.Date"/>
        <property name="operateUserAlias" column="operateUserAlias" type="string"/>
        <property name="operateUserName" column="operateUserName" type="string"/>
        <property name="operateApi" column="operateApi" type="string"/>
        <property name="operateLego" column="operateLego" type="string"/>
        <property name="input" column="input" type="string"/>
        <property name="output" column="output" type="string"/>
        <property name="content" column="content" type="string"/>
        <property name="operateOi" column="operateOi" type="string"/>
    </class>

    <!-- Task-->
    <class name="com.greatbee.core.bean.task.Task" table="ty_task" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="alias" column="alias" type="string"/>
        <property name="group" column="group" type="string"/>
        <property name="cron" column="cron" type="string"/>
        <property name="status" column="status" type="string"/>
        <property name="createDate" column="createDate" type="java.util.Date"/>
        <property name="apiAlias" column="apiAlias" type="string"/>
    </class>

    <!-- Task Log-->
    <class name="com.greatbee.core.bean.task.TaskLog" table="ty_task_log" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="createDate" column="createDate" type="java.util.Date"/>
        <property name="taskId" column="taskId" type="integer"/>
        <property name="result" column="result" type="string"/>
        <property name="timeConsuming" column="timeConsuming" type="long"/>
        <property name="begin" column="begin" type="java.util.Date"/>
        <property name="finish" column="finish" type="java.util.Date"/>
        <property name="remark" column="remark" type="string"/>
    </class>

    <!-- Auth Type-->
    <class name="com.greatbee.core.bean.auth.AuthType" table="ty_auth_type" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="alias" column="alias" type="string"/>
        <property name="judgeAPIAlias" column="judgeAPIAlias" type="string"/>
        <property name="noAuthErrorCode" column="noAuthErrorCode" type="long"/>
    </class>
</hibernate-mapping>