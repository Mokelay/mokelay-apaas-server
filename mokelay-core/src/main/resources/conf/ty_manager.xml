<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    <!-- API Manager-->
    <bean id="apiManager" class="com.greatbee.core.manager.ext.SimpleAPIManager" parent="abstractBasicManager"/>

    <!-- API Lego Manager-->
    <bean id="apiLegoManager" class="com.greatbee.core.manager.ext.SimpleAPILegoManager" parent="abstractBasicManager"/>

    <!-- Lego Manager-->
    <bean id="legoManager" class="com.greatbee.core.manager.ext.SimpleLegoManager" parent="abstractBasicManager"/>

    <!-- Lego Attribute Manager-->
    <bean id="legoAttributeManager" class="com.greatbee.core.manager.ext.SimpleLegoAttributeManager"
          parent="abstractBasicManager"/>

    <!-- InputFieldManager-->
    <bean id="inputFieldManager" class="com.greatbee.core.manager.ext.SimpleInputFieldManager"
          parent="abstractBasicManager"/>

    <!-- InputFieldDescribeManager-->
    <bean id="inputFieldDescribeManager" class="com.greatbee.core.manager.ext.SimpleInputFieldDescribeManager"
          parent="abstractBasicManager"/>

    <!-- OutputFieldManager-->
    <bean id="outputFieldManager" class="com.greatbee.core.manager.ext.SimpleOutputFieldManager"
          parent="abstractBasicManager"/>

    <!-- OutputFieldDescribe Manager-->
    <bean id="outputFieldDescribeManager" class="com.greatbee.core.manager.ext.SimpleOutputFieldDescribeManager"
          parent="abstractBasicManager"/>

    <!-- FileStorageManager-->
    <bean id="fileStorageManager" class="com.greatbee.core.manager.ext.SimpleFileStorageManager"
          parent="abstractBasicManager"/>

    <!-- userManager-->
    <bean id="userManager" class="com.greatbee.core.manager.ext.SimpleUserManager"
          parent="abstractBasicManager"/>
    <!-- appManager-->
    <bean id="appManager" class="com.greatbee.core.manager.ext.SimpleAppManager"
          parent="abstractBasicManager"/>
    <!-- appRoleManager-->
    <bean id="appRoleManager" class="com.greatbee.core.manager.ext.SimpleAppRoleManager"
          parent="abstractBasicManager"/>
    <!-- userAppRoleManager-->
    <bean id="userAppRoleManager" class="com.greatbee.core.manager.ext.SimpleUserAppRoleManager"
          parent="abstractBasicManager"/>

    <!-- pageManager-->
    <bean id="pageManager" class="com.greatbee.core.manager.ext.SimplePageManager"
          parent="abstractBasicManager"/>

    <!-- LogManager-->
    <bean id="logManager" class="com.greatbee.core.manager.ext.SimpleLogManager"
          parent="abstractBasicManager"/>

    <!-- CodeManager-->
    <bean id="codeManager" class="com.greatbee.core.manager.ext.SimpleCodeManager"
          parent="abstractBasicManager"/>

    <!-- Task Manager-->
    <bean id="taskManager" class="com.greatbee.core.manager.ext.SimpleTaskManager" parent="abstractBasicManager"/>
    <!-- Task Log Manager-->
    <bean id="taskLogManager" class="com.greatbee.core.manager.ext.SimpleTaskLogManager" parent="abstractBasicManager"/>

    <!-- Auth Type Manager-->
    <bean id="authTypeManager" class="com.greatbee.core.manager.ext.SimpleAuthTypeManager"
          parent="abstractBasicManager"/>
</beans>